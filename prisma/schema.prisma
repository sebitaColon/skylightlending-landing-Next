generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  name      String @db.VarChar(255)
  email     String @unique @db.VarChar(255)
  password  String @db.VarChar(255)
  id        Int    @id @default(autoincrement())
  last_name String @db.VarChar(255)
}
